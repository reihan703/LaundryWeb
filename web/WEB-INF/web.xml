<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <welcome-file-list>
        <welcome-file>HomePage.jsp</welcome-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>HomePageServlet</servlet-name>
        <servlet-class>Servlet.HomePageServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AboutUsPageServlet</servlet-name>
        <servlet-class>Servlet.AboutUsPageServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PricePageServlet</servlet-name>
        <servlet-class>Servlet.PricePageServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ContactUsPageServlet</servlet-name>
        <servlet-class>Servlet.ContactUsPageServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AccountPageServlet</servlet-name>
        <servlet-class>Servlet.AccountPageServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginPageServlet</servlet-name>
        <servlet-class>Servlet.LoginPageServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogoutServlet</servlet-name>
        <servlet-class>Servlet.LogoutServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditPageServlet</servlet-name>
        <servlet-class>Servlet.EditPageServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DashboardServlet</servlet-name>
        <servlet-class>Servlet.DashboardServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PaymentHistoryServlet</servlet-name>
        <servlet-class>Servlet.PaymentHistoryServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>HomePageServlet</servlet-name>
        <url-pattern>/HomePageServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AboutUsPageServlet</servlet-name>
        <url-pattern>/AboutUsPageServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PricePageServlet</servlet-name>
        <url-pattern>/PricePageServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ContactUsPageServlet</servlet-name>
        <url-pattern>/ContactUsPageServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AccountPageServlet</servlet-name>
        <url-pattern>/AccountPageServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginPageServlet</servlet-name>
        <url-pattern>/LoginPageServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogoutServlet</servlet-name>
        <url-pattern>/LogoutServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditPageServlet</servlet-name>
        <url-pattern>/EditPageServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DashboardServlet</servlet-name>
        <url-pattern>/DashboardServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PaymentHistoryServlet</servlet-name>
        <url-pattern>/PaymentHistoryServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
